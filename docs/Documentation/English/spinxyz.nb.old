Cell[CellGroupData[{
Cell["spinxyz", "ObjectName"],

Cell[TextData[{
  "\[FilledSmallSquare] ",
  StyleBox["spinxyz[", "MR"],
  StyleBox["c", "TI"],
  StyleBox["]", "MR"],
  " returns the ",
  StyleBox["x", "IT"],
  ", ",
  StyleBox["y", "IT"],
  ", and ",
  StyleBox["z", "IT"],
  " components of the S=1/2 spin operator for the level described by the \
operator ",
  StyleBox["c", "TI"],
  ".\n\[FilledSmallSquare] ",
  StyleBox["spinx[", "MR"],
  StyleBox["c", "TI"],
  StyleBox["]", "MR"],
  " returns the ",
  StyleBox["x", "IT"],
  " component of the spin operator.\n\[FilledSmallSquare] ",
  StyleBox["spiny[", "MR"],
  StyleBox["c", "TI"],
  StyleBox["]", "MR"],
  " returns the ",
  StyleBox["y", "IT"],
  " component of the spin operator.\n\[FilledSmallSquare] ",
  StyleBox["spinz[", "MR"],
  StyleBox["c", "TI"],
  StyleBox["]", "MR"],
  " returns the ",
  StyleBox["z", "IT"],
  " component of the spin operator.\n\[FilledSmallSquare] ",
  StyleBox["spinss[", "MR"],
  StyleBox["c", "TI"],
  StyleBox["]", "MR"],
  " returns the ",
  Cell[BoxData[
      \(TraditionalForm\`S\^2\)]],
  " total spin operator for the level.\n\[FilledSmallSquare] ",
  StyleBox["spinall[", "MR"],
  StyleBox["c", "IT"],
  StyleBox["]", "MR"],
  " returns the ",
  StyleBox["x", "IT"],
  ", ",
  StyleBox["y", "IT"],
  ", and ",
  StyleBox["z", "IT"],
  " components of the spin operator, as well as the ",
  Cell[BoxData[
      \(TraditionalForm\`S\^2\ \(\(operator\)\(.\)\)\)]],
  "\n\[FilledSmallSquare] ",
  StyleBox["spinplus[", "MR"],
  StyleBox["c", "TI"],
  StyleBox["]", "MR"],
  " returns the + component of the spin operator,",
  StyleBox[" ", "Notes"],
  Cell[BoxData[
      \(TraditionalForm\`\(S\_+\) = S\_x\)]],
  "+i ",
  Cell[BoxData[
      \(TraditionalForm\`S\_y\)]],
  ".\n\[FilledSmallSquare] ",
  StyleBox["spinminus[", "MR"],
  StyleBox["c", "TI"],
  StyleBox["]", "MR"],
  " returns the - component of the spin operator,",
  StyleBox[" ", "Notes"],
  Cell[BoxData[
      \(TraditionalForm\`\(S\_-\) = S\_x\)]],
  "-i ",
  Cell[BoxData[
      \(TraditionalForm\`S\_y\)]],
  "."
}], "Usage"],

Cell[TextData[{
  "\[FilledSmallSquare] Operator ",
  StyleBox["c", "TI",
